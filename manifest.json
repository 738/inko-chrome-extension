{
    "manifest_version": 2,
    "name": "Inko 한영타변환기",
    "version": "0.1.1",
    "description": "한영타 변환 라이브러리 Inko를 사용한 한영타 변환기",
    "homepage_url": "https://inko.holy.kiwi",
    "browser_action": {
        "default_title": "inko",
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/inko_128.png"
        }
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}